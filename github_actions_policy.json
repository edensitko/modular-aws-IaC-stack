{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "ec2:*",
        "iam:*",
        "autoscaling:*",
        "elasticloadbalancing:*",
        "cloudwatch:*",
        "s3:*",
        "rds:*",
        "dynamodb:*",
        "lambda:*",
        "apigateway:*",
        "cloudformation:*",
        "cloudfront:*",
        "route53:*",
        "ecs:*",
        "eks:*",
        "logs:*",
        "events:*",
        "sns:*",
        "sqs:*",
        "kms:*",
        "secretsmanager:*",
        "ssm:*",
        "organizations:*",
        "sts:*",
        "support:*",
        "tag:*",
        "cloudtrail:*",
        "config:*",
        "iam:CreateServiceLinkedRole",
        "iam:PutRolePolicy",
        "iam:CreateRole",
        "iam:AttachRolePolicy",
        "iam:PassRole"
      ],
      "Resource": "*"
    }
  ]
}
